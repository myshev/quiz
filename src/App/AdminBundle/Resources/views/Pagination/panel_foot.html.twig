<div class="row dataTables_wrapper">
    <div class="col-xs-6 col-left">
        <div class="dataTables_info" style="border: 0">
        {% if totalCount > 0 %}
            Результаты: {{ firstItemNumber }} — {{ lastItemNumber }} из {{ totalCount }}
        {% endif %}
        </div>
    </div>
    <div class="col-xs-6 col-right">
        <div class="dataTables_paginate" style="border: 0">
        {{ include('AdminBundle:Pagination:pagination.html.twig') }}
        </div>
    </div>
</div>
